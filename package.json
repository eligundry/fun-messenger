{
    "name": "fun-messenger",
    "version": "0.0.0",
    "description": "My attempt at making an app with the Parcel Bundler, React, and Typescript",
    "main": "src/index.html",
    "dependencies": {
        "@types/node-fetch": "^1.6.9",
        "node-fetch": "^2.1.2",
        "openpgp": "^3.0.9",
        "react": "^16.3.2",
        "react-dom": "^16.3.2",
        "react-redux": "^5.0.7",
        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0",
        "socket.io-client": "^2.1.0"
    },
    "devDependencies": {
        "node-sass": "^4.9.0",
        "parcel-bundler": "^1.8.1",
        "parcel-plugin-eslint": "^1.0.4",
        "tslint": "^5.10.0",
        "tslint-config-airbnb": "^5.8.0",
        "tslint-react": "^3.5.1",
        "typescript": "^2.8.3"
    },
    "scripts": {
        "serve": "parcel src/index.html -t browser -d dist/ -o dist/index.html --out-file index.html",
        "build": "parcel build src/index.html -t browser -d dist/ --out-file dist/index.html",
        "docker-build": "parcel build src/index.html -d /usr/share/nginx/html -t browser --out-file index.html",
